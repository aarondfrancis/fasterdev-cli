{
  "name": "fasterdev",
  "version": "0.1.3",
  "description": "Install AI coding assistant skills and rules from faster.dev",
  "type": "module",
  "bin": {
    "fasterdev": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/cli.ts --format esm --dts",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc --noEmit",
    "test": "node --test --import tsx \"tests/**/*.test.ts\" \"tests/**/*.test.js\""
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "fast-glob": "^3.3.2",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "semver": "^7.6.3",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.7.1",
    "tsup": "^8.1.0",
    "tsx": "^4.15.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ai",
    "claude",
    "cursor",
    "codex",
    "skills",
    "rules",
    "coding-assistant"
  ],
  "license": "MIT"
}
